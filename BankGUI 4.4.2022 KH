public class BankGUI extends javax.swing.JFrame {
    class Account {
    String name;
    float balance;
    float debt;
    public Account(String name, float balance, float debt) {
        this.name = name;
        this.balance = balance;
        this.debt = debt;
    }
    public float getBalance(){
        return balance;
    }
}
class CheckingAccount extends Account {
    public CheckingAccount(String name, float balance, float debt){
        super(name, balance, debt);
    }
    public float deposit(float num){
        balance += num;
        return balance;
    }
}
