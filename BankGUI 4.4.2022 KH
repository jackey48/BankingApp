    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        float num;
        float result;
        CheckingAccount a1 = new CheckingAccount("Harry", 100, 0); 
        num = Float.parseFloat(DepositText.getText());
        result = a1.deposit(num);
        jTextField1.setText(String.valueOf(result));
    }                                        

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        float num1;
        num1 = Float.parseFloat(jTextField1.getText()) - Float.parseFloat(WithdrawText.getText());
        jTextField1.setText(String.valueOf(num1));
    }                                        

